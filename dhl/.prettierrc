{
  "arrowParens": "always",
  "bracketSameLine": false,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.svelte",
      "options": {
        "parser": "svelte"
      }
    }
  ],
  "plugins": ["prettier-plugin-svelte"],
  "printWidth": 120,
  "quoteProps": "as-needed",
  "semi": true,
  "singleQuote": false,
  "svelteSortOrder": "scripts-options-markup-styles",
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false
}
